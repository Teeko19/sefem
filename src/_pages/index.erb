---
layout: default
---

<s-section>
  <s-container class="w-1/3">
    <img src="<%= relative_url "images/logo/logotype.png" %>" alt="Se fem?">
    <h1 class="sr-only">Se fem?</h1>
  </s-container>
</s-section>

<s-section>
  <s-container class="max-w-screen-xl">
    <nav>
      <ul class="grid grid-cols-6 gap-6">
        <% collections.months.resources.each do |month| %>
          <li>
            <a href="<%= relative_url month %>">
              <img src="<%= relative_url "images/months/#{month.data.slug}.png" %>"
                   alt="<%= month.data.title %>"
              >
            </a>
          </li>
        <% end %>
      </ul>
    </nav>
  </s-container>
</s-section>
